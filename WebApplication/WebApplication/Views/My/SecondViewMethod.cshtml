@model List<string>

<h2>Отсортированный список команд (таблица)</h2>

<table class="table" style="width:20%">
    <thead>
        <tr>
            <th class="text-center">Наименование</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var team in Model)
        {
            <tr class="text-center">
                <td>@team</td>
            </tr>
        }
    </tbody>
</table>